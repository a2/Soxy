GCC_PREPROCESSOR_DEFINITIONS = NO_FANCY_RESOLVER HAVE_SOCKADDR_DL_STRUCT MACOSX HAVE_UINT32_T HAVE_SOCKLEN_T USE_THREAD=1
HEADER_SEARCH_PATHS = ${PODS_HEADERS_SEARCH_PATHS}
PODS_ROOT = ${SRCROOT}/Pods
ALWAYS_SEARCH_USER_PATHS = YES
PODS_HEADERS_SEARCH_PATHS = ${PODS_PUBLIC_HEADERS_SEARCH_PATHS}
PODS_BUILD_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/BuildHeaders" "${PODS_ROOT}/BuildHeaders/BlocksKit" "${PODS_ROOT}/BuildHeaders/FormatterKit" "${PODS_ROOT}/BuildHeaders/Reachability" "${PODS_ROOT}/BuildHeaders/libffi" "${PODS_ROOT}/BuildHeaders/polipo" "${PODS_ROOT}/BuildHeaders/srelay"
PODS_PUBLIC_HEADERS_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/BlocksKit" "${PODS_ROOT}/Headers/FormatterKit" "${PODS_ROOT}/Headers/Reachability" "${PODS_ROOT}/Headers/libffi" "${PODS_ROOT}/Headers/polipo" "${PODS_ROOT}/Headers/srelay"
OTHER_LDFLAGS = -ObjC -Wl,-no_compact_unwind -framework MessageUI -framework SystemConfiguration